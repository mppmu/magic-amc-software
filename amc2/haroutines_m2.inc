int imin(int a, int b){
  return( (a<b)? a : b );
}

int imax(int a, int b){
  return( (a>b)? a : b );
}

float min(float a, float b){
  return( (a<b)? a : b );
}

float max(float a, float b){
  return( (a>b)? a : b );
}

double dmax(double a, double b){
  return( (a>b)? a : b );
}

